integrator = sd
nsteps = 50000
dt = 0.001

comm_mode = linear
comm_grps = system

gen-vel = yes
gen-temp = 300

cutoff-scheme = Verlet
nstlist = 10
ns_type = grid

nstlog = 5000
nstenergy = 5000
nstxout-compressed = 0

vdw-type = Cut-off
rvdw = 1.0

coulombtype = pme
fourierspacing = 0.1
pme-order = 4
rcoulomb = 1.0

constraint-algorithm = lincs
constraints = hbonds

tcoupl = v-rescale
ld-seed = 48456
tc-grps = Water non-Water
tau-t = 0.5 0.5
ref-t = 300 300

pull = yes
pull-nstxout = 50
pull-ncoords = 1
pull-ngroups = 2
pull-group1-name = ethanol_pull
pull-group2-name = water
pull-coord1-type = umbrella
pull-coord1-geometry = distance
pull-coord1-dim = Y N N 
pull-coord1-groups = 1 2
pull-coord1-start = no
pull-coord1-init = 3.90
pull-coord1-rate = 0.0
pull-coord1-k = 1000
pull-group1-pbcatom = 1387
