}

@article{stefaniukIsotopeEffectsObserved2022,
  title = {Isotope Effects Observed in Diluted {{D2O}}/{{H2O}} Mixtures Identify {{HOD-induced}} Low-Density Structures in {{D2O}} but Not {{H2O}}},
  author = {Stefaniuk, Anna and Gawinkowski, Sylwester and Golec, Barbara and Gorski, Aleksander and Szutkowski, Kosma and Waluk, Jacek and Pozna{\'n}ski, Jaros{\l}aw},
