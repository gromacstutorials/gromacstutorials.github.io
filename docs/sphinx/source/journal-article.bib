@article{mohottalalageEffectsIonicGroup2022,
  title = {Effects of {{Ionic Group Distribution}} on the {{Structure}} and {{Dynamics}} of {{Amorphous Polymer Melts}}},
  author = {Mohottalalage, Supun S. and Aryal, Dipak and Thurston, Bryce A. and Grest, Gary S. and Perahia, Dvora},
