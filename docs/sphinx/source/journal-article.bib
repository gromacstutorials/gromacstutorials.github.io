@article{liangStructuralDynamicalProperties2023,
  title = {Structural and {{Dynamical Properties}} of {{H2O}} and {{D2O}} under {{Confinement}}},
  author = {Liang, Chenxing and Rayabharam, Archith and Aluru, N. R.},
  year = {2023},
