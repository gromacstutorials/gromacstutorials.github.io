  author = {Singh, Shitanshu Pratap and Masiewicz, El{\.z}bieta and Singh, Rajendra Kumar and Chaurasia, Sujeet Kumar and Kruk, Danuta},
  year = {2024},
  month = nov,
  journal = {The Journal of Chemical Physics},
