@article{salazar-canoHydrogenBondsMethane2016,
  title = {Hydrogen Bonds in Methane--Water Clusters},
  author = {{Salazar-Cano}, Juan-Ram{\'o}n and {Guevara-Garc{\'i}a}, Alfredo and Vargas, Rubicelia and Restrepo, Albeiro and Garza, Jorge},
  year = {2016},
