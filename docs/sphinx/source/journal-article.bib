@article{hastingsSteamIsothermsCO2024,
  title = {Steam {{Isotherms}}, {{CO}} {\textsubscript{2}} /{{H}} {\textsubscript{2}} {{O Mixed-Gas Isotherms}}, and {{Single-Component CO}} {\textsubscript{2}} and {{H}} {\textsubscript{2}} {{O Diffusion Rates}} in {{CALF-20}}},
