@article{horinekPeptideAdsorptionHydrophobic2008,
  title = {Peptide Adsorption on a Hydrophobic Surface Results from an Interplay of Solvation, Surface, and Intrapeptide Forces},
  author = {Horinek, D. and Serr, A. and Geisler, M. and Pirzer, T. and Slotta, U. and Lud, S. Q. and Garrido, J. A. and Scheibel, T. and Hugel, T. and Netz, R. R.},
  year = {2008},
  month = feb,
  journal = {Proceedings of the National Academy of Sciences},
  volume = {105},
