@article{oktavianGasAdsorptionFramework2024,
  title = {Gas Adsorption and Framework Flexibility of {{CALF-20}} Explored via Experiments and Simulations},
  author = {Oktavian, Rama and Goeminne, Ruben and Glasby, Lawson T. and Song, Ping and Huynh, Racheal and Qazvini, Omid Taheri and {Ghaffari-Nik}, Omid and Masoumifard, Nima and Cordiner, Joan L. and Hovington, Pierre and Van Speybroeck, Veronique and Moghadam, Peyman Z.},
