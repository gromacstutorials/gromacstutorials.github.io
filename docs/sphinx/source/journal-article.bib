  file = {/home/simon/snap/zotero-snap/common/Zotero/storage/X9H27RLK/Dertinger et al. - 2007 - Two‐Focus Fluorescence Correlation Spectroscopy A New Tool for Accurate and Absolute Diffusion Meas.pdf}
