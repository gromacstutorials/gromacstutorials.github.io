@article{billemontAdsorptionCarbonDioxide2013,
  title = {Adsorption of {{Carbon Dioxide}}, {{Methane}}, and {{Their Mixtures}} in {{Porous Carbons}}: {{Effect}} of {{Surface Chemistry}}, {{Water Content}}, and {{Pore Disorder}}},
  shorttitle = {Adsorption of {{Carbon Dioxide}}, {{Methane}}, and {{Their Mixtures}} in {{Porous Carbons}}},
  author = {Billemont, Pierre and Coasne, Benoit and De Weireld, Guy},
  year = {2013},
  month = mar,
  journal = {Langmuir},
  volume = {29},
