@article{locheTransferableIonForce2021,
  title = {Transferable {{Ion Force Fields}} in {{Water}} from a {{Simultaneous Optimization}} of {{Ion Solvation}} and {{Ion}}--{{Ion Interaction}}},
  author = {Loche, Philip and Steinbrunner, Patrick and Friedowitz, Sean and Netz, Roland R. and Bonthuis, Douwe Jan},
  year = {2021},
