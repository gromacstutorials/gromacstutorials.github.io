@article{blasExcessPropertiesLennardJones2002,
  title = {Excess Properties of {{Lennard-Jones}} Binary Mixtures from Computer Simulation and Theory},
  author = {BLAS, FELIPE J. and FUJIHARA, {\relax ICHIRO}},
  year = {2002},
  month = sep,
  journal = {Molecular Physics},
