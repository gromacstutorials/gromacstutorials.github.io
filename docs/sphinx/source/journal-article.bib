@article{riahiIdentifyingShortLongtime2019,
  title = {Identifying Short- and Long-Time Modes of the Mean-Square Displacement: {{An}} Improved Nonlinear Fitting Approach},
  shorttitle = {Identifying Short- and Long-Time Modes of the Mean-Square Displacement},
  author = {Riahi, M. K. and Qattan, I. A. and Hassan, J. and Homouz, D.},
