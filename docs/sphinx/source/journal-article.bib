@article{agirreHybridOrganosilicaMembranes2014,
  title = {Hybrid Organosilica Membranes and Processes: {{Status}} and Outlook},
  shorttitle = {Hybrid Organosilica Membranes and Processes},
  author = {Agirre, Ion and Arias, Pedro L. and Castricum, Hessel L. and Creatore, Madriana and Ten Elshof, Johan E. and Paradis, Goulven G. and Ngamou, Patrick H.T. and Van Veen, Henk M. and Vente, Jaap F.},
